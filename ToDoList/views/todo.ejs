<html>
    <head>
        <title>
                ToDo List
        </title>

        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
        <script src= "/todo-list.js" rel="ajax-file"></script>
        <link rel="stylesheet" href="./style.css">
    </head>
    <body>
        <div>
            <form>
                <input type="text" name="item" required/>
                <button type="submit">Add</button>
            </form>
            <ul>
             <% for(var i=0; i<todos.length;i++){%><li><%=todos[i].item %></li><%}%>
            </ul>
        </div>
        <script type = "text/javascript" src= "/todo-list.js" rel="ajax-file"></script>
        
    </body>
</html>